{
  "expo": {
    "name": "Viva sem Tabaco",
    "slug": "viva-sem-tabaco",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4375b4"
    },
    "notification": {
      "icon": "./assets/images/logo_sobre.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "br.com.vivasemtabaco",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "br.com.vivasemtabaco",
      "permissions": [
        "USE_FINGERPRINT"
      ],
      "useNextNotificationsApi": true
    }
  }
}
